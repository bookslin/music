<!--components/song-item-v1/song-item-v1.wxml-->
<view class="item" bindtap="onSongItemTap">
    <image class="image" src="{{itemData.al.picUrl}}" mode=""/>
    <view class="info">
        <view class="name"> {{itemData.name}} </view>
        <view class="singer"> {{itemData.ar[0].name}} </view>
    </view>
    <view class="arrow">
        <van-icon name="arrow" color='#888' size='32rpx' />
    </view>
</view>
